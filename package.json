{
  "name": "gcs-cache",
  "version": "1.0.0",
  "description": "Cache your workload to a Google Cloud Storage bucket",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "package": "npm run build && ncc build lib/main.js -m -o dist/main && ncc build lib/post.js -m -o dist/post",
    "test": "jest --passWithNoTests",
    "release": "np --message=\"chore: v%s\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:x00real/gcs-cache.git"
  },
  "keywords": [
    "actions",
    "gcs",
    "google-cloud",
    "cache"
  ],
  "author": "Viet-Anh Duong <vietanhs0817@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/x00real/gcs-cache/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        85
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{md,yml,html,css,scss,json}": "prettier --write"
  },
  "homepage": "https://github.com/x00real/gcs-cache#readme",
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/semver": "7.3.13",
    "@types/tmp": "0.2.3",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "@vercel/ncc": "0.38.3",
    "eslint": "9.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-helpers": "2.0.1",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-sonarjs": "3.0.4",
    "eslint-plugin-unused-imports": "4.1.4",
    "jest": "30.0.5",
    "jest-circus": "30.0.5",
    "lint-staged": "16.1.4",
    "np": "10.2.0",
    "prettier": "3.6.2",
    "ts-jest": "29.0.5",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/github": "6.0.1",
    "@actions/glob": "0.5.0",
    "@google-cloud/storage": "6.9.4",
    "semver": "7.3.8",
    "tmp-promise": "3.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
